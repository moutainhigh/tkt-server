<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration   PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <classPathEntry
            location="E:\maven\apache-maven-3.3.9\repo\mysql\mysql-connector-java\5.1.5\mysql-connector-java-5.1.5.jar"/>
    <context id="MysqlContext" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <!--<plugin type="tk.mybatis.mapper.generator.MapperPlugin">-->
        <!--<property name="mappers" value="BaseMapper"/>-->
        <!--</plugin>-->
        <plugin type="org.mybatis.generator.plugins.LomboxPlugin">
            <property name="hasLombok" value="true"/>
        </plugin>
        <!-- 注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/><!-- 是否取消注释 -->
            <property name="suppressDate" value="true"/> <!-- 是否生成注释代时间戳 -->
        </commentGenerator>

        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection driverClass="org.gjt.mm.mysql.Driver"
                        connectionURL="jdbc:mysql://10.24.126.37:3306/tktcenter?useUnicode=true&amp;characterEncoding=UTF-8&amp;allowMultiQueries=true"
                        userId="tktcenteruser" password="TktCenter.135!">
        </jdbcConnection>

        <!-- 生成模型的包名和位置 -->
        <javaModelGenerator targetPackage="com.mtl.cypw.msg.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成映射文件的包名和位置 -->
        <sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.mtl.cypw.msg.mapper"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!-- 要生成哪些表 -->
        <table tableName="cy_msg_template_config" domainObjectName="MsgTemplateConfig"
               enableInsert="false" enableSelectByPrimaryKey="true"
               enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"
               enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false">
            <generatedKey column="id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="cy_msg_template_variable" domainObjectName="MsgTemplateVariable"
               enableInsert="false" enableSelectByPrimaryKey="true"
               enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"
               enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false">
            <generatedKey column="id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="cy_msg_send_record" domainObjectName="MsgSendRecord"
               enableInsert="false" enableSelectByPrimaryKey="true"
               enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"
               enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false">
            <generatedKey column="id" sqlStatement="mysql" identity="true"/>
        </table>
        <table tableName="cy_msg_send_receiver" domainObjectName="MsgSendReceiver"
               enableInsert="false" enableSelectByPrimaryKey="true"
               enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"
               enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false">
            <generatedKey column="id" sqlStatement="mysql" identity="true"/>
        </table>
    </context>
</generatorConfiguration>

